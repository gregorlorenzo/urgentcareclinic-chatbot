    <script>
        $.ajax({
            'url': "https://urgentcareclinic.onrender.com/api/users/currentuser",
            'method': "GET",
            'contentType': 'application/json',
            'success': function (data) {
                $("#current-user").text(`Current User: ${data.username}`);
            },
            'error': function (error) {
                console.log(error)
            }
        })
        document.addEventListener('DOMContentLoaded', function () {
            var calendarEl = document.getElementById('calendar');
            var calendar = new FullCalendar.Calendar(calendarEl, {
                timeZone: 'UTC',
                themeSystem: 'bootstrap5',
                initialView: 'dayGridMonth',
                height: "auto",
                dayMaxEvents: true,
                events: 'https://urgentcareclinic.onrender.com/api/appointments/calendarappointments'
            });
            calendar.render();
        });
    </script>
