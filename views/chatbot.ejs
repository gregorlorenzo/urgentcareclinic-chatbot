	<!-- Edit Modal -->
	<div class="modal fade" id="editInfoModal" tabindex="-1" aria-labelledby="editModalLabel" aria-hidden="true"
		data-bs-backdrop="static" data-bs-keyboard="false">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<h5 class="modal-title" id="editModalLabel">Chatbot Information</h5>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body" id="info-edit">
					<form>
						<div class="form-group" style="margin-bottom: 20px" ;>
							<label for="infoName"><strong>Information Name</strong></label>
							<input type="text" class="form-control" id="infoName" value="" readonly>
						</div>
						<div class="form-group" style="margin-bottom: 20px" ;>
							<label for="infoDescription"><strong>Description</strong></label>
							<textarea maxlength="150" class="form-control" id="infoDescription" style="height: 200px"
								required></textarea>
						</div>
						<div class="modal-footer">
							<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
							<button type="submit" class="btn btn-primary text-white" id="submitChanges">Save
								changes</button>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>

	<!-- Add Modal -->
