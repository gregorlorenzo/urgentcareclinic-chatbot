			</div>
		</div>
	</div>




	<!-- Javascript -->
	<script src="assets/plugins/popper.min.js"></script>
	<script src="assets/plugins/bootstrap/js/bootstrap.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.3/moment-with-locales.min.js"
		integrity="sha512-vFABRuf5oGUaztndx4KoAEUVQnOvAIFs59y4tO0DILGWhQiFnFHiR+ZJfxLDyJlXgeut9Z07Svuvm+1Jv89w5g=="
		crossorigin="anonymous" referrerpolicy="no-referrer"></script>
	<script type="text/javascript" charset="utf8"
		src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
	<script type="text/javascript" charset="utf8"
		src="https://cdn.datatables.net/1.12.1/js/dataTables.bootstrap5.min.js"></script>


	<!-- Page Specific JS -->
	<script src="assets/js/app.js"></script>
	<script>
		$.ajax({
			'url': "https://urgentcareclinic.onrender.com/api/users/currentuser",
			'method': "GET",
			'contentType': 'application/json',
			'success': function (data) {
				$("#current-user").text(`Current User: ${data.username}`);
			},
			'error': function (error) {
				console.log(error)
			}
		})
		$.ajax({
			'url': "https://urgentcareclinic.onrender.com/api/botsettings/",
			'method': "GET",
			'contentType': 'application/json',
			'success': function (data) {
				$('#table-info').dataTable({
					"aaData": data,
					"columns": [
						{ "data": "informationName" },
						{ "data": "description" },
						{ "data": null, render: function (data, type, full) { return '<button type="button" class="btn btn-secondary btn-sm text-white" id="editInfo">Edit</button>&nbsp<button type="button" class="btn btn-danger btn-sm text-white view_data" onclick="removeInfo()">Delete</button>' } }
					]
				})
			}
		})

		$(document).ready(function () {
			$(document).on('click', '#addInfo', function () {
				$('#addInfoModal').modal('show');
				$('#addInfoModal').off('hidden');
				$("#createInfo").one('click', function (e) {
					var info = {
						informationName: $('#addInfoModal').find('#infoName').val(),
						description: $('#addInfoModal').find('#infoDescription').val()
					}

					$.ajax({
						method: "POST",
						url: 'https://urgentcareclinic.onrender.com/api/botsettings/createinfo',
						data: JSON.stringify(info),
						contentType: "application/json",
						success: function (data) {
							$('#addInfoModal').modal('hide');
							alertify.notify('Successfully added information!', 'success', 2, function () {
								location.reload();
							});
						},
						error: function (jqXHR, textStatus, errorThrown) {
							console.log(textStatus, errorThrown)
						}
					})
					e.stopImmediatePropagation();
				})
			})
		})

		$(document).ready(function () {
			$(document).on('click', '#editInfo', function () {
				var infoName = $(this).closest('tr').find("td:first").text();
				var oldDescription = $(this).closest('tr').find('td:nth-child(2)').text();

				$('#editInfoModal').modal('show');
				$("#infoName").val(infoName);
				$("#infoDescription").val(oldDescription);

				$('#editInfoModal').off('hidden');
				$("#submitChanges").one('click', function (e) {
					e.preventDefault();
					var description = $('#editInfoModal').find('#infoDescription').val();

					$.ajax({
						method: "PUT",
						url: 'https://urgentcareclinic.onrender.com/api/botsettings/updateinfo?informationName=' + infoName + '&description=' + description,
						success: function (data) {
							$('#editInfoModal').modal('hide');
							alertify.notify('Successfully updated information!', 'success', 2, function () {
								location.reload();
							});
						}
					})
					e.stopImmediatePropagation();
				})
			})
		})

		function removeInfo() {
			var table = document.getElementById('info-table'), rIndex;
			var informationName = ""

			for (var i = 0; i < table.rows.length; i++) {
				table.rows[i].onclick = function () {
					rIndex = this.rowIndex;
					informationName = this.cells[0].innerHTML

					$.ajax({
						method: "DELETE",
						url: "https://urgentcareclinic.onrender.com/api/botsettings/deleteinfo?informationName=" + informationName,
						contentType: "application/json",
						success: function (data) {
							alertify.notify('Information removed!', 'success', 2, function () {
								location.reload();
							});
						}
					})
				}
			}
		}
	</script>
</body>

</html>